{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "useDefineForClassFields": false,
      "declaration": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "inlineSources": true,
      "isolatedModules": true,
      "lib": ["esnext", "dom", "dom.iterable"],
      "module": "esNext",
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "noImplicitAny": false,
      "rootDir": ".",
      "skipLibCheck": true,
      "strict": true,
      "sourceMap": true,
      "target": "esnext",
      "baseUrl": ".",
      "paths": {
        "*": ["./types/*", "*"],
        "@websublime/ws-essential/*": ["./src/*"],
        "@reduxjs/toolkit": ["../../node_modules/@reduxjs/toolkit/dist/index.d.ts"],
        "unstorage": ["../../node_modules/unstorage/dist/index.d.ts"],
        "https://esm.sh/v96/unstorage@0.5.6/es2022/drivers/localstorage.js": ["../../node_modules/unstorage/dist/drivers/localstorage.d.ts"],
        "https://esm.sh/v96/unstorage@0.5.6/es2022/drivers/memory.js": ["../../node_modules/unstorage/dist/drivers/memory.d.ts"]
      },
      "declarationDir": "./declarations",
      "removeComments": false,
      "incremental": true,
      "composite": true
    },
    "buildOptions": {"force": true},
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "types/**/*.d.ts"],
    "exclude": ["tmp/**", "src/**/*.spec.ts"],
    "extends": "../../tsconfig.base.json",
    "references": [
      {
        "path": "../../libs/vite"
      },
      {
        "path": "../ws-sublime"
      }
    ]
  }
